@page
@model OdeToFood.Pages.Restaurants.ListModel
@{
}

<h1>Restaurants</h1>

<table class="table">
    @foreach (var restaraunt in Model.Restaurants)
    {
        <tr>
            <td>@restaraunt.Name</td>
            <td>@restaraunt.Location</td>
            <td>@restaraunt.Cuisine</td>
        </tr>
    }
</table>


<div>@Model.Message</div>
